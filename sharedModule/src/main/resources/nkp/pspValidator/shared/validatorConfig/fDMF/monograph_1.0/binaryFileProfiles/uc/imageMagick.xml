<binaryFileProfile
        xmlns="http://www.nkp.cz/validatorEpublikaci/1.0/binaryFileProfile"
        validatorVersion="1.0"
        dmf="monograph_1.0"
>
    <fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?m)^Error.*$</regexp>
                    <regexp>(?ms)identify:((?!identify).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</binaryFileProfile>