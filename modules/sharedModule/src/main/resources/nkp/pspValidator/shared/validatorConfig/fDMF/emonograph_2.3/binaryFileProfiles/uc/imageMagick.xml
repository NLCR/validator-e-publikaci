<binaryFileProfile
        xmlns="http://www.nkp.cz/validatorEpublikaci/2.2.1/binaryFileProfile"
        validatorVersion="2.2.1"
        dmf="emonograph_2.3"
>
    <fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?m)^Error.*$</regexp>
                    <regexp>(?ms)identify:((?!identify).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</binaryFileProfile>